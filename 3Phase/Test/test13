PROGRAM test13;
	FUNCTION DEGREE(START,NODE:INTEGER;NEXT,ADJ,DEST,LINK,INDEG,OUTDEG:ARRAY[1..20] OF INTEGER):INTEGER;
	VAR PTR,PTRA,PTRB:INTEGER;
	BEGIN
		PTR:=START;
		WHILE(PTR<>NULL) DO
			BEGIN
				INDEG[PTR]:=0;
				OUTDEG[PTR]:=0;
				PTR:=PTR+1
			END;
		PTRA:=START;
		WHILE(PTRA<>START) DO
			BEGIN
				PTRB:=ADJ[PTRA];
				WHILE(PTRB<>NULL) DO
					BEGIN
						OUTDEG[PTRA]:=OUTDEG[PTRA]+1;
						INDEX:=DEST[PTRB];
						INDEG[INDEX]:=INDEG[INDEX]+1;
						PTRB:=LINK[PTRB]
					END;
				PTRA:=NEXT[PTRA]
			END
	END
	BEGIN
	WRITE('INPROGRAM')
	END

	
				
		
