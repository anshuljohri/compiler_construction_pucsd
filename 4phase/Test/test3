PROGRAM CHECK;
	PROCEDURE HELLO1 (A,B : INTEGER);
		PROCEDURE HELLO2 (C,D:INTEGER);
		   VAR A2,B2 : INTEGER;
		   BEGIN
			A2:=B2;
			B2:=4567;
		IF A=3
			THEN C:=3.567
			ELSE E:=2367
			WRITE('INHELLO')
		   END
		VAR A3,B3:INTEGER;
		BEGIN
		    A2:=A3;
		    WRITE('INHELLO')
		END
	VAR A4,B4:INTEGER;
	BEGIN
	   WRITE('INCHECK')
	END
